
    <% flash.each do |name, msg| %>
  <div class="container">    
      <div class="alert alert-success alert-dismissable fade show" role="alert">
        <button type="button" class="btn-close btn-close1 justify-align-right" data-bs-dismiss="alert" aria-label="Close"></button>
        <%= msg %>

        </div>
  </div>
    <% end %>